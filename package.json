{
  "name": "react-nhost",
  "version": "0.5.0",
  "description": "Nhost React components and hooks for SDK >= v4",
  "license": "MIT",
  "homepage": "https://www.npmjs.com/package/react-nhost",
  "bugs": {
    "url": "https://github.com/dohomi/react-nhost/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dohomi/react-nhost.git"
  },
  "author": "Dominic Garms <djgarms@gmail.com>",
  "funding": {
    "type": "buymeacoffee",
    "url": "https://buymeacoffee.com/dohomi"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "play": "vite --config playground/vite.config.ts",
    "test": "vitest",
    "check-types": "tsc --noEmit",
    "release": "bumpp && pnpm publish",
    "prepublishOnly": "yarn run build",
    "eslint": "eslint . --fix"
  },
  "peerDependencies": {
    "@nhost/nhost-js": ">=4 <5",
    "@simplewebauthn/browser": ">=13 <14",
    "react": ">=18.0.0 <20",
    "react-dom": ">=18.0.0 <20"
  },
  "devDependencies": {
    "@nhost/nhost-js": "^4.1.0",
    "@simplewebauthn/browser": "^13.2.2",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@tsconfig/strictest": "^2.0.7",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "bumpp": "^10.3.1",
    "eslint": "9.38.0",
    "eslint-plugin-query": "0.10.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "happy-dom": "^20.0.10",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tsdown": "^0.16.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "8.46.2",
    "vite": "npm:rolldown-vite@^7.1.20",
    "vitest": "^4.0.7"
  },
  "packageManager": "yarn@4.10.3"
}
